@using zic_dotnet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    @Html.CssDoc("master.css")
    <link rel="shortcut icon" href='@Html.ImgUrl("logo.ico")' type="image/x-icon" />
    <script type="text/javascript">        var glo_path = '@Html.ScriptUrl("")'</script>
    <script type="text/javascript" src='@Html.ScriptUrl("zic-js/sea.js")' data-main='@Html.ScriptUrl("init")'></script>
    @RenderSection("head")
</head>
<body>
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="brand-dark" href="/">ZKdao</a>
                <ul class="nav">
                    <li>@Html.ActionLink("首页", "Index", "Home")</li>
                    <li>@Html.ActionLink("商品库", "Index", "Account")</li>
                    <li>@Html.ActionLink("客户端", "Index", "Account")</li>
                    <li>@Html.ActionLink("关于我们", "About", "Home")</li>
                </ul>
                <form class="navbar-search pull-right">
                <input type="text" class="search-query" placeholder="搜索">
                </form>
                <ul class="nav pull-right">
                    @Html.Partial("_LogOnPartial")
                </ul>
            </div>
        </div>
    </div>
    <div class="navbar-fixed-top container logohack">
        <a class="brand-light" href="/">ZKdao</a>
    </div>
    <div id="main" class="row">
        <div class="container">
            <div class="span9">
                @RenderBody()
            </div>
            <div class="span3">
                @RenderSection("sidebar")
            </div>
        </div>
    </div>
    <div id="footer">
        <div class="container">
            <span class="SiteFooter">Copyright &copy; 2012-2012, Apworks Project. All rights reserved.
            </span>
        </div>
    </div>
</body>
</html>
